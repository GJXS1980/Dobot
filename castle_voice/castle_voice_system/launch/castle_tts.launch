<?xml version = "1.0"?>
<launch>
    <!--运行科大讯飞在线语音识别功能包-->
    <node pkg = "castle_voice_system" type = "castle_xf_tts_node" name = "castle_xf_tts_node" output="screen">
        <!--修改appid，创建每个服务都有唯一的appid-->
        <param name="appid" value="appid = 5b6d44ed, work_dir = ." />
        <!--会话参数-->
        <param name="speech_param" value="voice_name = 	catherine, text_encoding = utf8, sample_rate = 16000, speed = 10, volume = 50, pitch = 50, rdn = 1" />
    </node>
</launch>